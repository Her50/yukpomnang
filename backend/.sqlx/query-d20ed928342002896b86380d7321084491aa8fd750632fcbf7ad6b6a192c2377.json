{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            id,\n            CASE \n                WHEN TRIM(COALESCE(nom, '') || ' ' || COALESCE(prenom, '')) = '' \n                THEN split_part(email, '@', 1)\n                ELSE TRIM(COALESCE(nom, '') || ' ' || COALESCE(prenom, ''))\n            END as nom_complet,\n            email,\n            is_provider,\n            gps,\n            photo_profil,\n            avatar_url,\n            created_at\n        FROM users \n        WHERE id = $1\n        ",
