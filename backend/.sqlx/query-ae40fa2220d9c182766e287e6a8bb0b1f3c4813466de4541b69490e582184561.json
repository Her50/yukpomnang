  "query": "\n            SELECT id, user_id, offre, besoin, quantite_offerte, quantite_requise, \n                   gps_fixe_lat, gps_fixe_lon, don, created_at\n            FROM echanges \n            WHERE statut = 'en_attente' \n              AND id != $1\n              AND created_at > NOW() - INTERVAL '7 days'\n            ORDER BY created_at DESC \n            LIMIT $2\n            ",
